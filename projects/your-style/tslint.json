{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "your",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "your",
      "kebab-case"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "triple-equals": false,
    "no-inferrable-types": false,
    "no-trailing-whitespace": false,
    "no-output-on-prefix": false
  }
}
